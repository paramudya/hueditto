{
  "manifest_version": 3,
  "name": "HueDitto",
  "version": "1.0",
  "description": "Fixes your shitty codes before it breaks the engines, ditto",

    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://127.0.0.1:5000/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Text Processor"
    }
}