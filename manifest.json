{
  "manifest_version": 3,
  "name": "Text Reader Extension",
  "version": "1.0",
  "description": "Reads text from textboxes when activated",
  "permissions": [
      "activeTab",
      "scripting"
  ],
  "host_permissions": [
      "<all_urls>"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_title": "Text Reader"
  }
}